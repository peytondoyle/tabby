{
  "name": "tabby",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preflight": "node scripts/preflight.mjs",
    "dev": "vite --host --port 5173 --strictPort",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "db:push": "supabase db push",
    "db:new": "supabase migration new",
    "db:seed": "supabase db seed",
    "gen:types": "supabase gen types typescript --linked > src/lib/database.types.ts",
    "types:check": "tsc --noEmit",
    "lint:strict": "eslint . --max-warnings=0",
    "dead:exports": "ts-prune -p tsconfig.tsprune.json --ignore Tests,stories,.d.ts",
    "dead:files": "unimported",
    "dead:all": "npm run types:check && npm run lint:strict && npm run dead:exports && npm run dead:files"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/jetbrains-mono": "^5.2.6",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.85.5",
    "@types/node": "^24.3.0",
    "autoprefixer": "^10.4.21",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.541.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "tailwindcss": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "unimported": "^1.31.0",
    "vite": "^7.1.2"
  }
}

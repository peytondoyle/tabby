{
  "name": "tabby",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preflight": "node scripts/preflight.mjs",
    "dev": "node scripts/dev-full.mjs",
    "dev:api": "vercel dev --listen 3000 --yes --debug",
    "dev:full": "node scripts/dev-full.mjs",
    "dev:vite": "vite --host --port 5173 --strictPort",
    "build": "vite build",
    "build:with-types": "tsc -b && vite build",
    "profile": "vite --mode profile",
    "profile:start": "node scripts/start-profile.mjs",
    "analyze": "node scripts/analyze-bundle.mjs",
    "lint": "eslint .",
    "lint:styles": "eslint src/**/*.{ts,tsx} --rule 'tabby-ui/no-hardcoded-styles: error' --rule '@typescript-eslint/no-explicit-any: off' --rule 'react-hooks/exhaustive-deps: off' --max-warnings=0",
    "validate:styles": "npm run lint:styles && echo 'âœ… All style validations passed!'",
    "test:smoke": "playwright test tests/smoke-test.spec.ts --reporter=line",
    "test:ui": "playwright test tests/ui-sandbox.spec.ts --reporter=html",
    "test:performance": "playwright test --config=playwright.performance.config.ts",
    "test:lighthouse": "playwright test tests/lighthouse-share.spec.ts",
    "lighthouse:ci": "lhci autorun",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "db:push": "supabase db push",
    "db:new": "supabase migration new",
    "db:seed": "supabase db seed",
    "gen:types": "supabase gen types typescript --linked > src/lib/database.types.ts",
    "types:check": "tsc --noEmit",
    "lint:strict": "eslint . --max-warnings=0",
    "dead:exports": "ts-prune -p tsconfig.tsprune.json --ignore Tests,stories,.d.ts",
    "dead:files": "unimported",
    "dead:all": "npm run types:check && npm run lint:strict && npm run dead:exports && npm run dead:files",
    "serve": "vite build && npx serve dist -l 5050",
    "dev:check": "curl -s http://127.0.0.1:3000/api/scan-receipt?health=1 && echo && curl -s http://127.0.0.1:3000/api/scan-receipt-health",
    "dev:check:preflight": "curl -i -X OPTIONS http://127.0.0.1:3000/api/scan-receipt -H 'Origin: http://localhost:5173' -H 'Access-Control-Request-Method: POST' -H 'Access-Control-Request-Headers: Content-Type'",
    "dev:diag:api": "echo '=== API Health Check ===' && curl -s http://127.0.0.1:3000/api/scan-receipt?health=1 && echo && echo && echo '=== CORS Preflight Test ===' && curl -i -X OPTIONS http://127.0.0.1:3000/api/bills/create -H 'Origin: http://localhost:5173' -H 'Access-Control-Request-Method: POST' -H 'Access-Control-Request-Headers: Content-Type' && echo && echo '=== Environment Diagnostics ===' && curl -s http://127.0.0.1:3000/api/dev/env-diag && echo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@dnd-kit/core": "^6.3.1",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/formidable": "^3.4.5",
    "@types/node": "^24.3.0",
    "@vercel/node": "^5.3.17",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "exifr": "^7.1.3",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.541.0",
    "nanoid": "^5.1.5",
    "openai": "^5.16.0",
    "pdfjs-dist": "^5.4.296",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "unimported": "^1.31.0",
    "vercel": "^46.1.1",
    "vite": "^7.1.2"
  }
}
